{
  "builds": [
    {
      "src": "bookmyseat/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9",
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "bookmyseat/wsgi.py"
    }
  ],
  "buildCommand": "python manage.py collectstatic --noinput"
}
